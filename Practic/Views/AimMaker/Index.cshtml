@model AimMakerViewModel
<div class="container">
    @foreach(var element in Model._AimMaker.ListAimItems)
    {
        <div class="alert alert-warning mt-3">
            <b>Задание:</b> @element.aim.Name<br/>
        </div>
        <textarea class="text_size_extra">
            Пояснение:
            @element.aim.TextField
        </textarea>
        <textarea class="text_size_extra text_size_aim">
            Задание:
            @element.aim.AimTextField
        </textarea>
    }
    <iframe width="100%" height="475" src="https://dotnetfiddle.net/Widget/BCEuFq" frameborder="0"></iframe>
    <a class="btn btn-danger mb-5" asp-controller="Home" asp-action="Index">Завершить</a>
    <hr/>
</div>
